@model List<HW8.Models.Turnover>

@{
    ViewData["Title"] = "ShowTurn!importanter";
    Layout = "_Layout";
}
<div>
    <table class="table table-striped mb-0">
        <thead style="background-color: #002d72; color:#FFF!important">
            <tr>
                <th>نام حساب</th>
                <th>بدهکار</th>
                <th>بستانکار</th>
                <th>تاریخ تراکنش</th>
                <th>توضیحات</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Debit</td>
                    <td>@item.Credit</td>
                    <td>@item.TransactionTime</td>
                    <td>@item.Description</td>
                </tr>
            }
        </tbody>
    </table>
</div>